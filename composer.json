{
    "name": "spanjaan/wn-bloghub-plugin",
    "type": "winter-plugin",
    "version": "1.0.0",
    "description": "Extends Winter's Blog extension with tag, comment, additional archives and more.",
    "keywords": [
        "winter", 
        "wintercms", 
        "blog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "S.p. Anjaan",
            "email": "spanjaan@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 7.4 || >= 8.0",
        "composer/installers": "~1.0",
        "gregwar/captcha": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
